<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:divider="?android:attr/dividerHorizontal"
    android:paddingEnd="12dp"
    android:paddingStart="12dp"
    android:showDividers="middle|end"
    android:stretchColumns="1"
    tools:context="demaikel.sistemasexpertos.MainActivity"
    tools:showIn="@layout/activity_main">


    <TableRow>

        <TextView
            android:layout_width="match_parent"
            android:paddingTop="10dp"
            android:paddingBottom="20dp"
            android:text="Estándares y protocolos que utilizamos" />
    </TableRow>


    <RelativeLayout>


        <TextView
            android:layout_marginRight="1dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableLeft="@mipmap/estandares_protocolos"
            android:gravity="center_horizontal"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />


    </RelativeLayout>


    <TableRow>

        <TextView
            android:layout_width="match_parent"
            android:paddingTop="20dp"
            android:paddingBottom="20dp"
            android:text="Socios Estratégicos" />
    </TableRow>


    <RelativeLayout>


        <TextView
            android:layout_marginRight="1dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableLeft="@mipmap/entel"
            android:gravity="center_horizontal"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:paddingBottom="20dp"/>
            </RelativeLayout>

    <RelativeLayout>
        <TextView
            android:layout_marginRight="1dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableLeft="@mipmap/grifols"
            android:gravity="center_horizontal"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"/>

    </RelativeLayout>

</TableLayout>